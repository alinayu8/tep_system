<!DOCTYPE html>
<html>
  <head>
    <title>DataCat</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>
  <body>
    <%# if logged_in? %>
        <%= render :partial => "partials/nav" %>
    <%#end%>
    
    <%# GRID LAYOUT %>
    <div class=row>
      <div class="col s2"></div>
      <div class="col s8">
        <%= yield %>
      </div>
      <div class="col s2"></div>
    </div>

  </body>
</html>
