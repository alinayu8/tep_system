<h1>Schools</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Phone</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @schools.each do |school| %>
      <tr>
        <td><%= school.organization_name__c %></td>
        <td><%= school.phone__c %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
