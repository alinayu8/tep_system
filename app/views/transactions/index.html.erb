<h1>Orders#index</h1>
<% if @orders.empty? %>
    <p>No orders in the system right now</p>
<% else %>
<table class="striped highlight"> 
    <thead>
        <th>Name</th>
        <th>School</th>
        <th>Shopping Date</th>
        <th>Item</th>
        <th>Quantity</th>
        <th>Semester</th>
        <th>Date Entered</th>
    </thead>
    <% for order in @orders %>
        <tr>

        </tr>
    <% end %>
</table>
<% end %>